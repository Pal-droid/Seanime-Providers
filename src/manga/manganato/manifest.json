{
  "id": "mangakakalove",
  "name": "MangaKakalove",
  "description": "MangaKakalove is a manga provider for Seanime",
  "manifestURI": "https://raw.githubusercontent.com/Pal-droid/Seanime-Providers/development/src/manga/manganato/manifest.json",
  "version": "1.0.0",
  "author": "pal",
  "icon": "",
  "type": "manga-provider",
  "language": "javascript",
  "lang": "en",
  "payloadURI": "https://raw.githubusercontent.com/Pal-droid/Seanime-Providers/development/src/manga/manganato/provider.js",
  "userConfig": {
    "requiresConfig": false,
    "version": 1,
    "fields": [
      {
        "name": "domain",
        "label": "Preferred Domain",
        "type": "select",
        "default": "https://www.mangakakalove.com",
        "options": [
          { "label": "mangakakalove.com (Default)", "value": "https://www.mangakakalove.com" },
          { "label": "natomanga.com", "value": "https://www.natomanga.com" },
          { "label": "mangabats.com", "value": "https://www.mangabats.com" },
          { "label": "nelomanga.net", "value": "https://www.nelomanga.net" },
          { "label": "mangakakalot.gg", "value": "https://www.mangakakalot.gg" }
        ]
      }
    ]
  }
}
