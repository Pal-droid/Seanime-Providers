{
  "id": "mangabuddy-test",
  "name": "MangaBuddy Test",
  "description": "MangaBuddy is a manga provider for Seanime",
  "manifestURI": "https://raw.githubusercontent.com/Pal-droid/Seanime-Providers/refs/heads/main/src/test-extensions/manifest.json",
  "version": "1.1.0",
  "author": "pal",
  "type": "manga-provider",
  "language": "javascript",
  "lang": "en",
  "payloadURI": "https://raw.githubusercontent.com/Pal-droid/Seanime-Providers/refs/heads/main/src/test-extensions/mangabuddy/test",

  "userConfig": {
    "requiresConfig": true,
    "version": 1,
    "fields": [
      {
        "name": "domain",
        "label": "Preferred Domain",
        "type": "select",
        "default": "https://mangabuddy.com",
        "options": [
          { "label": "mangabuddy.com", "value": "https://mangabuddy.com" },
          { "label": "mangaforest.me", "value": "https://mangaforest.me" },
          { "label": "mangacute.com", "value": "https://mangacute.com" },
          { "label": "mangaxyz.com", "value": "https://mangaxyz.com" },
          { "label": "mangapuma.com", "value": "https://mangapuma.com" },
          { "label": "mangamirror.com", "value": "https://mangamirror.com" },
          { "label": "truemanga.com", "value": "https://truemanga.com" },
          { "label": "mangafab.com", "value": "https://mangafab.com" },
          { "label": "manhwatube.com", "value": "https://manhwatube.com" }
        ]
      }
    ]
  }
}
